{
    "trailingComma": "es5",
    "tabWidth": 4,
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "plugins": ["prettier-plugin-tailwindcss", "@ianvs/prettier-plugin-sort-imports"],
    "tailwindFunctions": ["clsx", "cva", "tw"],
    "importOrder": ["<TYPES>", "<TYPES>^[.]", "^@core/(.*)$", "^@server/(.*)$", "", "^@ui/(.*)$", "", "^[./]", "<BUILT_IN_MODULES>","", "<THIRD_PARTY_MODULES>", "^(?!.*[.]css$)[./].*$", ".css$"],
    "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"],
    "importOrderTypeScriptVersion": "5.3.2"
}